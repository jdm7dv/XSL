<Northwind xmlns:updg="urn:schemas-microsoft-com:xml-updategram">
  <updg:header>
    <updg:param name="CustomerID"></updg:param>
    <updg:param name="CompanyName"></updg:param>
  </updg:header>
  <updg:sync>
    <updg:before>
      <Customers CustomerID="$CustomerID"/>
    </updg:before>
    <updg:after>
      <Customers CompanyName="$CompanyName"/>
    </updg:after>
  </updg:sync>
</Northwind>

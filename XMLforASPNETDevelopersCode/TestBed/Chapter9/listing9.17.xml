<ROOT xmlns:updg="urn:schemas-microsoft-com:xml-updategram">
    <updg:sync>
        <updg:before>
            <TABLENAME [updg:id="value"] col="value" col="value"…../>
        </updg:before>
        <updg:after>
            <TABLENAME [updg:id="value"] [updg:at-identity="value"]
             col="value" col="value"…../>
        </updg:after>
    </updg:sync>
</ROOT>
